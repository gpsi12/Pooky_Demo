    <declare-styleable name="BaseTheme"><attr format="color" name="actionBarIconColor"/><attr format="dimension" name="actionBarInsetStart"/><attr format="dimension" name="spinnerBarInsetStart"/><attr format="reference" name="popupItemBackground"/><attr format="reference" name="photoItemForeground"/><attr format="reference" name="photoItemForegroundBorderless"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="action_settings">Settings</string>
